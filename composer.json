{
    "name": "rawilk/randallwilk.dev",
    "type": "project",
    "description": "My personal website.",
    "keywords": [
        "Randall Wilk",
        "personal website",
        "rawilk",
        "framework",
        "laravel",
        "docs"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-heroicons": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "khatabwedaa/blade-css-icons": "^1.1",
        "knplabs/github-api": "^3.8",
        "laravel/framework": "^9.19",
        "laravel/horizon": "^5.9",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "league/commonmark": "^2.3",
        "livewire/livewire": "^2.10",
        "predis/predis": "^2.0",
        "rawilk/laravel-base": "^0.4.9",
        "rawilk/laravel-breadcrumbs": "^4.0",
        "rawilk/laravel-casters": "^3.0",
        "rawilk/laravel-form-components": "^7.1",
        "spatie/fork": "^1.1",
        "spatie/laravel-login-link": "^1.1",
        "spatie/laravel-markdown": "^2.2",
        "spatie/laravel-menu": "^4.1",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/packagist-api": "^2.1",
        "spatie/sheets": "^1.9",
        "spatie/valuestore": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^1.2",
        "rawilk/laravel-stubs": "^1.1",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:meta --ansi",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan horizon:publish --ansi",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan custom-stub:publish --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "model-docs": "@php artisan ide-helper:models -R -W",
        "update-permissions": "@php artisan db:seed --class=UpdatePermissionSeeder --force",
        "test": "vendor/bin/pest -p",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
