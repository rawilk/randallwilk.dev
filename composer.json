{
    "name": "rawilk/randallwilk.dev",
    "type": "project",
    "description": "My personal website.",
    "keywords": [
        "Randall Wilk",
        "personal website",
        "rawilk",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "blade-ui-kit/blade-heroicons": "^0.2.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.5",
        "knplabs/github-api": "^2.15",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "leuverink/blade-shape-divider": "^2.0",
        "livewire/livewire": "^2.1",
        "myclabs/php-enum": "^1.7",
        "php-http/guzzle6-adapter": "^1.1",
        "rawilk/laravel-casters": "^1.0",
        "rawilk/laravel-form-components": "^1.4",
        "react/child-process": "^0.6.1",
        "react/promise": "^2.8",
        "spatie/commonmark-highlighter": "^2.1",
        "spatie/laravel-menu": "^3.6",
        "spatie/laravel-sitemap": "^5.8",
        "spatie/packagist-api": "^2.0",
        "spatie/sheets": "^1.7",
        "spatie/valuestore": "^1.2",
        "wyrihaximus/react-child-process-promise": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
